<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.funlap">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Funlap">
        <activity android:name=".ViewVideo"></activity>
        <activity android:name=".ViewFun" />
        <activity android:name=".ViewPoem" />
        <activity android:name=".UserHomeScreen" />
        <activity android:name=".ViewStory" />
        <activity android:name=".FunAddScreen" />
        <activity android:name=".VideoAddScreen" />
        <activity android:name=".PoemAddScreen" />
        <activity android:name=".StoryAddScreen" />
        <activity android:name=".AdminHomeScreen" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>